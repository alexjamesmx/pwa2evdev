{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended",
     "plugin:react/recommended",
      "plugin:react-hooks/recommended",
       "plugin:tailwindcss/recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react", "react-hooks", "tailwindcss"],
    "rules": {
    },
    "globals": {
      "process": true
    }
  
}  